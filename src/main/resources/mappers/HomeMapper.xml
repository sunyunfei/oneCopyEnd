<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bs.dao.HomeDao">
    <!--查询所有-->
    <select id="queryHomeData" parameterType="java.lang.String" resultType="com.bs.pojo.Home">
        SELECT *
        FROM home
        WHERE user_name=#{userName}
    </select>

</mapper>